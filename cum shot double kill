local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "CrazyNoob's fucker V7.9.7", HidePremium = false, SaveConfig = true, IntoEnable = false})


function MakeScriptHub()
    local Window = OrionLib:MakeWindow({Name = "CrazyNoob's Fucker", HidePremium = false, SaveConfig = true, IntroEnable = true, Introtext = "Script Hub"})
end

local Tab = Window:MakeTab({
	Name = "Tradescam",
	Icon = "rbxassetid://4483345998",
		PremiumOnly = false
})

